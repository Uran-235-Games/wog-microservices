{
  "name": "wog-socketio-gateway",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "gen:proto": "npx protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto.cmd --ts_proto_opt=esModuleInterop=true,forceLong=string,useOptionals=messages --ts_proto_out=./src/gen -I ../protos ../protos/proto/user-service/main.proto"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "ts-proto": "^2.7.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  }
}
